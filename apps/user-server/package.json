  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.8",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^3.12.0",
