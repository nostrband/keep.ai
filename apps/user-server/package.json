{
  "name": "@app/user-server",
  "version": "1.0.0",
  "description": "AI assistant proxy server with user management and billing",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "bin": {
    "user-server": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js server",
    "create-user": "node dist/cli.js create-user",
